<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AnimButton">

        <!-- R角度数 -->
        <attr name="animButtonR" format="integer" />
        <attr name="animButtonBgColor" format="color" />
        <attr name="animButtonDuration" format="integer" />

        <attr name="animButtonTxt" format="string" />
        <attr name="animButtonTxtSize" format="dimension" />
        <attr name="animButtonTxtColor" format="color" />

        <attr name="animButtonArcPadding" format="dimension" />
        <attr name="animButtonArcWidth" format="dimension" />
        <attr name="animButtonArcColor" format="color" />

    </declare-styleable>

    <declare-styleable name="DiyViewOne1">
        <attr name="oWidth" format="dimension" />
        <attr name="oBgColor" format="color" />
        <attr name="oColor" format="color" />
        <!-- 是否渐变 -->
        <attr name="oGradient" format="boolean" />
        <attr name="oGradientType">
            <enum name="oGradientLeftToRight" value="0x100" />
            <enum name="oGradientTopToBottom" value="0x200" />
        </attr>
        <attr name="oColor2" format="color" />
        <attr name="oMaxValue" format="integer" />
        <attr name="oValue" format="integer" />
        <attr name="oDuration" format="integer" />
    </declare-styleable>

</resources>